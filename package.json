{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@monaco-editor/react": "^3.3.0",
    "@umijs/hooks": "^1.8.0",
    "@umijs/preset-react": "^1.4.5",
    "@umijs/test": "^3.0.11",
    "bizcharts": "^3.5.8",
    "github-markdown-css": "^4.0.0",
    "lint-staged": "^10.0.7",
    "monaco-themes": "^0.3.3",
    "nprogress": "^0.2.0",
    "prettier": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-live-clock": "^4.0.5",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^12.2.1",
    "rough-charts": "^0.4.2-alpha.0",
    "umi": "^3.0.11",
    "yorkie": "^2.0.0"
  }
}
